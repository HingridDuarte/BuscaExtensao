package BuscaExtensao;

public class Aresta {

    private int id;
    private Vertice a = null;
    private Vertice b = null;

    public Aresta(Vertice a, Vertice b) {
        this.a = a;
        this.b = b;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    
    public void setB(Vertice b) {
        this.b = b;
    }
    
    public Vertice getA() {
        return a;
    }

    public void setA(Vertice a) {
        this.a = a;
    }

    public Vertice getB() {
        return b;
    }

}
